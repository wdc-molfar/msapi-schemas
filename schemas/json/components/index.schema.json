      {
       "$ref": "#/definitions/Reference",
       "description": "Посилання на налаштування обмінника"
      },
      {
       "title": "Підсхема \"exchange\"",
       "description": "Налаштування обміника",
       "subschema": "msapi.components.exchange",
       "type": "object",
       "additionalProperties": false,
       "patternProperties": {
        "^name$": {
         "title": "name",
         "description": "Ім'я обміника",
         "type": "string"
        },
        "^mode$": {
         "title": "mode",
         "description": "Тип обміника",
         "enum": [
          "fanout",
          "direct",
          "topic"
         ],
         "default": "fanout"
        },
        "^options$": {
         "title": "options",
         "description": "Налаштування обміника",
         "$ref": "./exchange-options"
        }
       }
      }
     ]
    }
   }
  },
  "^queues$": {
   "type": "object",
