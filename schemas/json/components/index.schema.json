      },
      {
       "subschema": "msapi.components.queue",
       "type": "object",
       "additionalProperties": false,
       "patternProperties": {
        "^name$": {
         "type": "string"
        },
        "^exchange$": {
         "oneOf": [
          {
           "$ref": "#/definitions/Reference"
          },
          {
           "$ref": "./exchange"
          }
         ]
        },
        "^options$": {
         "$ref": "./queue-options"
        }
       }
      }
     ]
    }
   }
  },
  "^schemas$": {
   "type": "object",
   "patternProperties": {
